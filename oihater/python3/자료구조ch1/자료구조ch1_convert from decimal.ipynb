   "source": [
    "def convert_from_decimal(num, base): # base -> 2이상 10이하\n",
    "    multiple, result = 1,0\n",
    "    while num>0:\n",
    "        result += num%base * multiple\n",
    "        multiple *= 10\n",
    "        num = num//base\n",
    "    return result"
   ]
  },
